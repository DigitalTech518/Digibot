{
	"help": [
		"**- Bot -**",
		"**- Restricted -**",
		"**- Fun -**",
		"**- Moderation -**",
		"**- Server -**",
		"**- Utility -**",
		"**- User -**",
		"**- Music -**",
		"**- Other -**"
	],
	"categories": {
		"music":{
			"commands":[
				"play                [song name]",
				"leave               [N/A]",
				"skip                [N/A]",
				"stop                [N/A]",
				"pause               [N/A]",
				"resume              [N/A]",
				"queue               [N/A]",
				"clearqueue          [N/A]",
				"forceskip           [N/A]"
			]
		},
		"utility":{
			"commands":[
				"purge               [amount]",
				"remindme            [duration][reminder]",
				"removereminder      [number]",
				"reminders           [N/A]",
				"temperature         [temperature][temp type][conv type]",
				"settime             [UTC offset]",
				"time                <user>",
				"addreactionrole     <message> <emoji> [role name]",
				"removereactionrole  <message>"
			]
		},
		"moderation":{
			"commands": [
				"ban                 <member>[reason]",
				"kick                <member>[reason]",
				"unban               [userID]",
				"mute                <member>[time][reason]",
				"permamute           <member>[reason]",
				"mutedrole           [N/A]",
				"unmute              <member>",
				"warn                <member>[reason]",
				"pardonwarn          <member>[number]",
				"modnote             <user>[note]",
				"pardonnote          <user>[note number (see order on list)]",
				"records             <user>"
			]
		},
		"server": {
			"commands": [
				"lock                <channel>",
				"unlock              <channel>",
				"setlogs             <channel>[general, moderation, voice, member, message, server]",
				"removelogs          <channel>[category]",
				"serverinfo          [N/A]",
				"slowmode            [time (s,m,h)]",
				"setjoinrole         [role]",
				"viewjoinroles       [N/A]",
				"removejoinrole      [role number (see order on list)]"
			]
		},
		"bot": {
			"commands": [
				"disablecommand      [command]",
				"enablecommand       [command]",
				"help                [category/command]",
				"info                [N/A]",
				"invite              [N/A]",
				"ping                [N/A]",
				"prefix              [prefix]",
				"support             [message]",
				"stats               [command]",
				"privacy             [N/A]"
			]
		},
		"restricted": {
			"commands": [
				"dm                  [memberID][message]",
				"botded              [N/A]",
				"say                 <channelID>[message]"
			]
		},
		"user": {
			"commands": [
				"pfp                 <member>",
				"profile             <member>"
			]
		},
		"fun": {
			"commands": [
				"8ball               [question]",
				"goddamn             [N/A]",
				"butterdog           [N/A]",
				"shut                [N/A]",
				"yikes               [N/A]",
				"pong                [N/A]",
				"unfunny             [N/A]",
				"hmm                 [N/A]",
				"google              [N/A]",
				"bean                <user>[reason]"
			]
		},
		"other": {
			"commands": [
				"embed               [title][description][thumbnailURL]"
			]
		}
	},
	"commands":{
		"play":{
			"Description:": "Play a song from youtube!",
			"Permission:": "None",
			"Usage:": "play [song name]"
		},
		"leave":{
			"Description:": "Have the bot leave the voice channel.",
			"Permission:": "None",
			"Usage:": "leave",
			"Aliases:": "||f*ckoff||"
		},
		"skip":{
			"Description:": "Skip to the next song!",
			"Permission:": "None",
			"Usage:": "skip"
		},
		"stop":{
			"Description:": "Stop the music.",
			"Permission:": "None",
			"Usage:": "stop"
		},
		"pause":{
			"Description:": "Pause the current song!",
			"Permission:": "None",
			"Usage:": "pause"
		},
		"resume":{
			"Description:": "Resume a previously paused song!",
			"Permission:": "None",
			"Usage:": "resume"
		},
		"queue":{
			"Description:": "See the songs currently in the queue",
			"Permission:": "None",
			"Usage:": "queue",
		},
		"clearqueue":{
			"Description": "Clear the current queue.",
			"Permission:": "Manage_messages",
			"Usage:": "clearqueue"
		},
		"forceskip":{
			"Description:": "Skip the current song without vote.",
			"Permission:": "Manage_messages",
			"Usage:": "forceskip"
		},
		"google":{
			"Description:": "Google be like.",
			"Permission:": "None",
			"Usage:": "google"
		},
		"removereactionrole":{
			"Description:": "Remove a previously set reaction role message.",
			"Permission:": "manage_roles",
			"Usage:": "removereactionrole <message>"
		},
		"addreactionrole":{
			"Description:": "Add a reaction roles to a message!",
			"Permission:": "manage_roles",
			"Usage:": "addreactionrole <message> <emoji> [role name]"
		},
		"help":{
			"Description:": "Get help about a category or command!",
			"Permission:": "None",
			"Usage:": "help [category/command]",
			"Aliases:": "list"
		},
		"reminders":{
			"Description:": "View your current reminders!",
			"Permission Required:": "None",
			"Usage:": "reminders"
		},
		"records":{
			"Description:": "Look at the warns, mutes, and notes of a user.",
			"Permission Required:": "manage_messages",
			"Usage:": "records <user>"
		},
		"setjoinrole":{
			"Description:": "Set a role for when a member joins the server!",
			"Permission Required:": "manage_roles",
			"Usage:": "setjoinrole [role]"
		},
		"viewjoinroles":{
			"Description:": "View your currently set join roles for your guild!",
			"Permission Required:": "manage_roles",
			"Usage:": "viewjoinroles"
		},
		"removejoinrole":{
			"Description:": "Remove a join role from your guild.",
			"Permission Required:": "manage_roles",
			"Usage:": "removejoinrole [role number (see order on list)]"
		},
		"modnote":{
			"Description:": "Add a note to any user, for any reason!",
			"Permission Required:": "manage_messages",
			"Usage:": "modnote <user> [note]" 
		},
		"pardonnote":{
			"Description:": "Pardon a set note for a user.",
			"Permission Required:": "manage_messages",
			"Usage:": "pardonnote <user> [note number (see order on records command)]"
		},
		"privacy":{
			"Description:": "Look at my privacy policy",
			"Permission Required:": "None",
			"Usage:": "privacy"
		},
		"kick":{
			"Description:": "Kicks a user out of the server.",
			"Permission Required:": "Permission: Ban Member",
			"Usage:": "kick <@user> [reason]"
		},
		"ban":{
			"Description:": "Bans a user from the server.",
			"Permission Required:": "Permission: Ban Member",
			"Usage:": "ban <@user> [reason]"
		},
		"permamute":{
			"Description:": "Permanently mute a user until manually unmuted.",
			"Permission Required:": "Permission: Ban Member",
			"Usage:": "permamute <@user> [reason]"
		},
		"unmute":{
			"Description:": "Unmute a user that is muted.",
			"Permission Required:": "Permission: Ban Member",
			"Usage:": "unmute <@user> [reason]"
		},
		"mute":{
			"Description:": "Mute a user for a specified amount of time.",
			"Permission Required:": "Permission: Ban Member",
			"Usage:": "mute <@user> [time (s,m,h,d,w)] [reason]"
		},
		"pfp":{
			"Description:": "Pull up the profile picture of yourself or another user",
			"Permission Required:": "None",
			"Usage:": "pfp <@user>",
			"Aliases:": "avatar, profile_picture"
		},
		"profile":{
			"Description:": "Look up your own profile or another's profile.",
			"Permission Required:": "None",
			"Usage:": "profile <@user>"
		},
		"warn":{
			"Description:": "Warn a user for breaking rules.",
			"Permission Required:": "manage_messages",
			"Usage:": "warn <@user> [reason]"
		},
		"ping":{
			"Description:": "Find out the latency.",
			"Permission Required:": "None",
			"Usage:": "ping",
			"Aliases:": "latency"
		},
		"prefix":{
			"Description:": "Change the prefix of the bot for your server.",
			"Permission Required:": "administrator",
			"Usage:": "prefix [prefix]"
		},
		"purge":{
			"Description:": "Delete a mass amount of messages",
			"Permission Required:": "manage_messages",
			"Usage:": "purge [number of messages]",
			"Aliases:": "delete, begone"
		},
		"remindme":{
			"Description:": "Set a dm reminder (or in channel of command if dms are off)",
			"Permission Required:": "None",
			"Usage:": "remindme [time(s,m,h,d,w)] [reminder]",
			"Aliases:": "reminder, remind"
		},
		"setlogs":{
			"Description:": "Set a channel to a certin category for audit logs. (General is a category that will get all audit categories)",
			"Permission Required:": "administrator",
			"Usage:": "setlogs <#channel> [general, moderation, voice, member, message, server]"
		},
		"bean":{
			"Description:": "Bean a user to bean them!",
			"Permission Required:": "manage_messages",
			"Usage:": "bean <@user> [reason]"
		},
		"goddamn":{
			"Description:": "Goddamn.",
			"Permission Required:": "None",
			"Usage:": "goddamn"
		},
		"eightball":{
			"Description:": "Ask the all mighty 8 ball a yes or no question!",
			"Permission Required:": "None",
			"Usage:": "8ball [question]",
			"Aliases:": "8ball"
		},
		"butterdog":{
			"Description:": "Butterdog",
			"Permission Required:": "None",
			"Usage:": "butterdog"
		},
		"shut":{
			"Description:": "Shut",
			"Permission Required:": "None",
			"Usage:": "shut"
		},
		"yikes":{
			"Description:": "Yikes",
			"Permission Required:": "None",
			"Usage:": "yikes"
		},
		"lock":{
			"Description:": "Prevent anyone from typing in a channel",
			"Permission Required:": "administrator",
			"Usage:": "lock <channel>"
		},
		"unlock":{
			"Description:": "Unlocks a perviously locked channel",
			"Permission Required:": "administrator",
			"Usage:": "unlock <channel>"
		},
		"pong":{
			"Description:": "Ping!",
			"Permission Required:": "None",
			"Usage:": "pong"
		},
		"embed":{
			"Description:": "Create an embed message with a title, description, and a thumbnail!",
			"Permission Required:": "None",
			"Usage:": "embed ['Title']['Description']['Thumbnail URL'] (single quotes need to be double quotes)"
		},
        "info":{
        	"Description:": "Show information about the bot.",
        	"Permission Required:": "None",
        	"Usage:": "botinfo",
        	"Aliases:": "botinfo"
        },
     	"serverinfo":{
        	"Description:": "Show information about the server.",
        	"Permission Required:": "None",
        	"Usage:": "serverinfo",
        	"Aliases:": "sinfo"
        },
        "unfunny":{
        	"Description:": "Unfunny dude",
        	"Permission Required:": "None",
        	"Usage:": "unfunny"
        },
        "pardonwarn":{
        	"Description:": "Remove a warning from a member. Warning is the warning you see in the command d!warnlist, the numbers go up to down (the top warning is the 1st warning).",
        	"Permission Required:": "manage_messages",
        	"Usage:": "pardonwarn <@user> [warning]"
        },
        "slowmode":{
        	"Description:": "Set the slowmode of a channel",
        	"Permission Required:": "administrator",
        	"Usage:": "slowmode [time in s,m,or h]",
        	"Aliases:": "slow"
        },
        "mutedrole":{
        	"Description:": "Make a 'Muted' role.",
        	"Permission Required:": "manage_roles",
        	"Usage:": "mutedrole",
        	"Aliases:": "mrole"
        },
        "invite":{
        	"Description:": "Invite the bot to your own server!",
        	"Permission Required:": "None",
        	"Usage:": "invite"
        },
        "temperature":{
        	"Description:": "Convert the temperature.",
        	"Permission Required:": "None",
        	"Usage:": "temperature [current temp] [type of temp (f or c or k)] [convert type (f or c or k)]"
        },
        "botded":{
        	"Description:": "Good luck with trying this >:)"
        },
        "stats": {
        	"Description": "See the stats of a command, or just see top commands used.",
        	"Permission Required:": "None",
        	"Usage:": "stats [command name]"
        },
        "removereminder":{
        	"Description:": "Remove a on-going reminder.",
        	"Permission Required:": "None",
        	"Usage:": "removereminder [which reminder (1 is the first remidner on list)]",
        	"Aliases:": "Rreminder"
        },
        "removelogs":{
        	"Description:": "Remove an all ready set log.",
        	"Permission Required:": "administrator",
        	"Usage:": "removelogs <channel> [category]"
        },
        "disablecommand":{
        	"Description:": "Disable a command serverwide.",
        	"Permission Required:": "administrator",
        	"Usage:": "disablecommand [command]"
        },
        "enablecommand": {
        	"Description:": "Re-enable a command that was previously disabled.",
        	"Permission Required:": "administrator",
        	"Usage:": "enablecommand [command]"
        },
        "support":{
        	"Description:": "Contact the owner of the bot for support!",
        	"Permission Required:": "None",
        	"Usage:": "support [message]"
        },
        "settime":{
        	"Description:": "Set your UTC offset",
        	"Permission Required:": "None",
        	"Usage:": "settime [UTC offset]"
        },
        "time": {
        	"Description:": "See your or another users time if they have used settime.",
        	"Permission Required:": "None",
        	"Usage:": "time <member>"
        }
	}
}